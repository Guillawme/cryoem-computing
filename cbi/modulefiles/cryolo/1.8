#%Module1.0

proc ModulesHelp { } {
global dotversion
puts stderr "\tcrYOLO, version 1.8.x"
}

module-whatis "Neural net particle picker. Documentation: https://cryolo.readthedocs.io/en/latest/"

conflict pyem epu-group-afis imod topaz cryodrgn cryoem-utilities kltpicker deepemhancer pymol starparser cryoef findmysequence cryocare eman2 model-angelo

# To avoid repetitions in this module file
set program cryolo
set version 1.8
set condaprefix /opt/miniconda3/envs
set prefix $condaprefix/$program-$version

setenv CRYOLOPATH $prefix/$program-$version
prepend-path PATH $prefix/bin
