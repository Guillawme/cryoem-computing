#%Module1.0

proc ModulesHelp { } {
global dotversion
puts stderr "\tUnison file synchronizer, version 2.53.3"
}

module-whatis "File synchronization tool. Documentation: https://github.com/bcpierce00/unison"

# To avoid repetitions in this module file
set program unison
set version 2.53.3
set prefix /opt/$program-$version

prepend-path PATH $prefix/bin
prepend-path MANPATH $prefix/man

